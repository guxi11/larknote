{
  "name": "Larknote",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A chrome extension which can enhance the feishu documentsâ€™ ability to take notes.",
  "homepage_url": "https://github.com/guxi11/larknote",
  "icons": {
    "16": "icons/icon180.png",
    "32": "icons/icon180.png",
    "48": "icons/icon180.png",
    "128": "icons/icon180.png"
  },
  "browser_action": {
    "default_title": "Larknote",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "default_locale": "en",
  "background": {
  "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.feishu.cn/*"],
      "js": ["js/content.js"]
    }
  ]
}
